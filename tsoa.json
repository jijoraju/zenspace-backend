{
  "entryFile": "bin/www.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["controllers/**/*.ts"],
  "spec": {
    "outputDirectory": "dist",
    "specVersion": 3,
    "specMerging": "recursive",
    "spec": {
      "info": {
        "title": "Zenspace Backend API",
        "version": "1.0.0"
      },
      "servers": [
        {
          "url" : "/",
          "description": "Backend api application for zenspace api"
        }
      ]
    }
  },
  "routes": {
    "routesDir": "routes"
  }
}
